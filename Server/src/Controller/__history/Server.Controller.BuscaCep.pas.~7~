unit Server.Controller.BuscaCep;

interface

uses
  Horse.GBSwagger,
  Commons.DTO.Erro,
  Commons.DTO.Cep,
  Server.Classes.Constantes,
  Server.Model.Cep.Interfaces;

type
  [SwagPath('buscarcep','Serviço para consulta de Ceps')]
  TControllerBuscaCep = class(THorseGBSwagger)
  public
    [SwagGET('{cep}','Digite o Cep.')]
    [SwagParamPath('cep','Cep')]
    [SwagResponse(200, TSetorDTO)]
    [SwagResponse(404, TCepDTO)]
    procedure ObterCep;

  end;

implementation

{ TControllerBuscaCep }

procedure TControllerBuscaCep.ObterCep;
begin

end;

end.
